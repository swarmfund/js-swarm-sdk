{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["babel-plugin-transform-builtin-extend", {
        "globals": [ "Error", "Array"]
    }]
  ]
}